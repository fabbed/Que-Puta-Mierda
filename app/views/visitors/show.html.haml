%h2= "Visitor \"#{@visitor.to_param}\""
- content_tag_for :div, @visitor do
  %ul
    %li
      %strong Email:
      =h @visitor.email
    %li
      %strong Returns:
      =h @visitor.returns
    %li
      %strong Country:
      =h @visitor.country_id
%ul
  %li= link_to 'Edit', edit_visitor_path(@visitor)
  %li= link_to 'Back', visitors_path